__all__ = [
    "version"
]

PCT_VERSION_MAJOR = "@PCT_VERSION_MAJOR@"
PCT_VERSION_MINOR = "@PCT_VERSION_MINOR@"
PCT_VERSION_PATCH = "@PCT_VERSION_PATCH@"
PCT_VERSION_HASH = "@PCT_VERSION_HASH@"

def version():
    return f"PCT {PCT_VERSION_MAJOR}.{PCT_VERSION_MINOR}.{PCT_VERSION_PATCH}{PCT_VERSION_HASH}"
